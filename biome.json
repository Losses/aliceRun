{
   "$schema": "https://biomejs.dev/schemas/1.7.3/schema.json",
   "organizeImports": {
      "enabled": true
   },
   "linter": {
      "enabled": true,
      "rules": {
         "recommended": true,
         "complexity": {
            "noForEach": "off"
         }
      }
   },
   "formatter": {
      "indentStyle": "space",
      "indentWidth": 3
   },
   "javascript": {
      "formatter": {
         "quoteStyle": "single"
      }
   }
}
